/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": ["jasmine"],
    "paths": {
      "@/core/*": ["src/app/core/*"],
      "@/features/*": ["src/app/features/*"],
      "@/shared/*": ["src/app/shared/*"],
      "@/environments/*": ["src/environments/*"]
    }
  },
  "include": ["src/**/*.spec.ts", "src/**/*.d.ts"]
}
